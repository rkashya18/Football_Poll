  "tableData": "{{\n  (() => {\n    const gid = Dropdown_Game.selectedOptionValue;\n    if (!gid) return [];\n\n    const usersById = Object.fromEntries(\n      (GetUsers.data || []).map(u => [String(u.UserID), u])\n    );\n\n    const votes = (GetVotesCurrent.data || [])\n      .filter(v => String(v.GameID) === String(gid));\n\n    const toVoteTimeMs = (vt) => {\n      if (vt === null || vt === undefined || vt === \"\") return NaN;\n\n      const s = String(vt).trim();\n\n      // Numeric timestamps\n      if (/^\\d+$/.test(s)) {\n        const n = Number(s);\n        return s.length >= 13 ? n : n * 1000;\n      }\n\n      // Datetime string already shifted by Appsmith → treat as UTC ms\n      const m = moment(s, [\"YYYY-MM-DD HH:mm:ss\", moment.ISO_8601], true);\n      return m.isValid() ? m.valueOf() : NaN;\n    };\n\n    const eligible = votes\n      .filter(v => [\"IN\", \"PAID\"].includes(String(v.Choice).toUpperCase()))\n      .sort((a, b) => {\n        const ta = toVoteTimeMs(a.VoteTime);\n        const tb = toVoteTimeMs(b.VoteTime);\n        if (ta !== tb) return ta - tb;\n        return Number(a.VoteOrder || 0) - Number(b.VoteOrder || 0);\n      })\n      .map((v, i) => {\n        const position = i + 1;\n        const u = usersById[String(v.UserID)] || {};\n        const isPaid = String(v.Choice).toUpperCase() === \"PAID\";\n\n        const baseMs = toVoteTimeMs(v.VoteTime);\n        const orderMs = Number(v.VoteOrder || 0);\n        const msPart = orderMs ? (orderMs % 1000) : 0;\n\n        // ✅ FINAL FIX: subtract IST offset added by Appsmith\n        const votedAtDisplay =\n          Number.isFinite(baseMs)\n            ? `${moment(baseMs)\n                .subtract(330, \"minutes\")\n                .format(\"DD-MMM HH:mm:ss\")}.${String(msPart).padStart(3, \"0\")}`\n            : v.VoteTime;\n\n        return {\n          Rank: position,\n          Slot: position <= 12 ? \"Playing\" : `WL-${position - 12}`,\n          User: String(v.UserID),\n          Paid: isPaid ? \"YES\" : \"NO\",\n          VotedAt: votedAtDisplay\n        };\n      });\n\n    return eligible;\n  })()\n}}\n",
